{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "akka": {
    "loggers": [
      "Akka.Logger.Extensions.Logging.LoggingLogger, Akka.Logger.Extensions.Logging"
    ],
    "loglevel": "INFO",
    "stdout-loglevel": "INFO",
    "actor": {
      "provider": "cluster"
    },
    "remote": {
      "dot-netty": {
        "tcp": {
          "hostname": "127.0.0.1",
          "port": 2551
        }
      }
    },
    "cluster": {
      "roles": [
        "aggregation"
      ],
      "seed-nodes": [
        "akka.tcp://aggregation-actor-system@127.0.0.1:2551"
      ],
      "downing-provider-class": "Akka.Cluster.SplitBrainResolver, Akka.Cluster",
      "split-brain-resolver": {
        "active-strategy": "keep-majority"
      },
      "sharding": {
        "number-of-shards": 100,
        "passivate-idle-entity-after": "5 m"
      }
    }
  }
}
